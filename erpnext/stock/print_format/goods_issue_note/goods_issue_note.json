 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">GOODS ISSUE NOTE</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">\r\n            {%- if doc.items[0].cost_center -%}\r\n                {{ doc.items[0].cost_center }}\r\n            {%- endif -%}\r\n        </p>\r\n    </div>\r\n   <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>GIN No:</strong></td>\r\n                <td>{{doc.name}}</td>\r\n                <td><strong>GIN Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            {%- if doc.items[0].material_request -%}\r\n            <tr>\r\n                <td><strong>Material Request No:</strong></td>\r\n                <td>{{ doc.items[0].material_request }}</td>\r\n                <td><strong>Material Request Date:</strong></td>\r\n                <td>\r\n\t               {{frappe.db.get_value(\"Material Request\", doc.items[0].material_request, \"transaction_date\")}}\r\n                </td>\r\n            </tr>\r\n            {%- endif -%}\r\n            <tr>\r\n                <td><strong>Vehicle No:</strong></td>\r\n                <td>{{doc.items[0].equipment}}</td>\r\n                <td><strong>Material Issue Title:</strong></td>\r\n                <td>{{doc.title}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Item Code</strong></th>\r\n                <th id=\"col\"><strong>Item Name</strong></th>\r\n                <th id=\"col\"><strong>UOM</strong></th>\r\n                <th id=\"col\"><strong>Qty Issued</strong></th>\r\n                <th id=\"col\"><strong>Amount</strong></th>\r\n            </tr>\r\n            <tr id=\"col\">\r\n                {% for row in doc.items%}\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.item_code}}</td>\r\n                <td id=\"col\">{{row.item_name}}</td>\r\n                <td id=\"col\">{{row.uom}}</td>\r\n                <td id=\"col\">{{row.qty}}</td>\r\n                <td id=\"col\">{{row.amount}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n            <tr id=\"col\">\r\n                <td id=\"col\" colspan=\"5\" style=\"text-align:right;\">\r\n                    <b>Total:</b>\r\n                </td>\r\n                <td id=\"col\">\r\n                    {{doc.total_incoming_value}}\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div id=\"in_word\">\r\n        <strong>Amount in Words: </strong> Ngultrum {{ (frappe.utils.money_in_words(doc.total_incoming_value))[3:] }}\r\n    </div>\r\n    <div style=\"padding-top: 100px; clear: both; font-size: small;\">\r\n        <div>Authorized Signatory</div>\r\n        <div style=\"padding-top: 80px; width: 25%; float: left; text-align:center;\">(Issued By) </div>\r\n        <div style=\"padding-top: 80px; width: 25%; float: left; text-align:center;\">(Checked By) </div>\r\n        <div style=\"padding-top: 80px; width: 25%; float: left; text-align:center;\">(Carrier) </div>\r\n        <div style=\"padding-top: 80px; width: 25%; float: left; text-align:center;\">(Received by) </div>\r\n        <div style=\"padding-top: 80px; text-align:center; clear: both;\">(Verified By) </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",

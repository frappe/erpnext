 "Incoming Mail Setting": "\u0906\u0928\u0947 \u0935\u093e\u0932\u0940 \u092e\u0947\u0932 \u0938\u0947\u091f\u093f\u0902\u0917", 
 "Login Id": "\u0906\u0908\u0921\u0940 \u0932\u0949\u0917\u093f\u0928", 
